{
  "version": "1.0.0",
  "scripts": {
    "commit": "cz",
    "release": "release-it",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "eslint": "eslint . --ext .vue,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/self-denial-cy/typescript-exercise.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/self-denial-cy/typescript-exercise/issues"
  },
  "homepage": "https://github.com/self-denial-cy/typescript-exercise#readme",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "gitlab": {
      "release": false
    },
    "github": {
      "release": false
    },
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/node": "^18.15.11",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.1",
    "prettier": "^2.8.7",
    "release-it": "^15.10.1",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "core-js": "^3.30.0"
  }
}
